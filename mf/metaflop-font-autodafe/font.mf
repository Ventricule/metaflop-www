% 2 dimension %
font_size:=12pt#;  % font size                  @hidden
ht#:=8pt#;        % box height                 @hidden
u#:=2pt#;        % unit widht 			    			@hidden
s#:=0u#;           % spacing                    @hidden

% 1 graisse %
px#:=1pt#;       % pen width 					$ 0 / 3

% 3 proportion %
cap#:=1ht#;      % cap heigt					@hidden

% export %
sidebearing:=1;    % sidebearings (0) off, (1) on for .otf export

input fontbase;    % now generate the font
input glyphs;

bye;
